{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@features/*": ["src/features/*"],
      "@pages": ["src/pages"],
      "@pages/*": ["src/pages/*"],
      "@store": ["src/store"],
      "@store/*": ["src/store/*"],
      "@config": ["src/config"],
      "@lib": ["src/lib"],
      "@lib/*": ["src/lib/*"],
      "@services": ["src/services"],
      "@services/*": ["src/services/*"],
      "@constants": ["src/constants"],
      "@constants/*": ["src/constants/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@i18n/*": ["src/i18n/*"]
    }
  }
}
